{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "RootWebProj": "../Jaydlc.Web",
  "PublishPath": "../Jaydlc.Web/bin/publish",
  "PublishArgs": "publish -c Release $RootWebProj -o $PublishPath",
  "UploadPath": "/tmp",
  "ArchiveExtractionPath": "/home/jaydlc/website/publish",
  "BackupsPath": "/home/jaydlc/website/backups"
}
